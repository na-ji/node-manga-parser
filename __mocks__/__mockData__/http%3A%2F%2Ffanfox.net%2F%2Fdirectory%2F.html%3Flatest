<script type="text/javascript">
$LAB.queueWait(function() {

    if(/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
        var hide = getCookie('MHNMB');
        $(".sidetip").show();
        $(".sidetip .sidetip-btn .app_link").click(function(){
        		if(/Android/i.test(navigator.userAgent)){
        			$(this).attr('href',$(this).data('android'));
        			return true;
        		}else if(/iPhone|iPad|iPod/i.test(navigator.userAgent)){
        			$(this).attr('href',$(this).data('apple'));
        			console.log($(this).data('apple'));
        			return true;
        		}else{
        			return false;
        		}
        		return false;
        });
        $(".sidetip .setting-icon").click(function(){
          $(".sidetip .sidetip-btn").toggleClass("slideout");
        });
    }

});


</script>

