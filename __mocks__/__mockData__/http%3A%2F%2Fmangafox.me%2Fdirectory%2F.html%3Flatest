<script type="text/javascript">
window.addEventListener('load', function() {
	var dialogLogin = document.getElementById('dialog-login');
	if (dialogLogin) {
		dialogLogin.addEventListener('click', function() {
			var xhr = new XMLHttpRequest,
				url = document.getElementById('loginform').action,
				data = new FormData,
				username = document.getElementById('navbar_username').value,
				password = document.getElementById('navbar_password').value;

			if (!username || !password) {
				// alert('username or password is required!');
				return false;
			}
			data.append('vb_login_username', username);
			data.append('vb_login_password', password);

			xhr.open('POST', url);

			xhr.onload = function() {
				var res = JSON.parse(xhr.responseText);
				switch(res.code) {
					case 0:
						window.location.reload(true);
						break;
					case 4:
						if (confirm(res.message)) {
							window.location.href = 'http://forums.mangafox.me/login.php?do=lostpw';
						}
						break;
					case 1:
					case 2:
					case 3:
					default:
						alert(res.message);
						break;
				}
			}
			xhr.send(data);
		});
	}
});
$LAB.queueWait(function() {

    if(/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
        var hide = getCookie('MHNMB');
        $(".sidetip").show();
        $(".sidetip .sidetip-btn .app_link").click(function(){
        		if(/Android/i.test(navigator.userAgent)){
        			$(this).attr('href',$(this).data('android'));
        			return true;
        		}else if(/iPhone|iPad|iPod/i.test(navigator.userAgent)){
        			$(this).attr('href',$(this).data('apple'));
        			console.log($(this).data('apple'));
        			return true;
        		}else{
        			return false;
        		}
        		return false;
        });
        $(".sidetip .setting-icon").click(function(){
          $(".sidetip .sidetip-btn").toggleClass("slideout");
        });
    }

});


</script>

