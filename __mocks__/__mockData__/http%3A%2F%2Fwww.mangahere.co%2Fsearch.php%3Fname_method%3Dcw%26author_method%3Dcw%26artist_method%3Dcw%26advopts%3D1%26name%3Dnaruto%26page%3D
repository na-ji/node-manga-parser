function addFavorite() {
	if (document.all) {
		try {
			window.external.addFavorite('http://www.mangahere.co', 'Manga Here - Read English Manga Free Online. Manga is Here!');
		} catch (e) {
			alert("Fail to add to your favorites, please press Ctrl + D to try again");
		}
	} else if (window.sidebar) {
		window.sidebar.addPanel('Manga Here - Read English Manga Free Online. Manga is Here!', 'http://www.mangahere.co', "");
	} else {
		alert("Fail to add to your favorites, please press Ctrl + D to try again");
	}
}

function setHomepage() {
	if (document.all) {
		document.body.style.behavior = 'url(#default#homepage)';
		document.body.setHomePage('http://www.mangahere.co');
	} else if (window.sidebar) {
		if (window.netscape) {
			try {
				netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
			} catch (e) {
				alert('This operation was rejected by your brower, please input “about:config”at address bar and turn "signed.applets.codebase_principal_support" to ture.');
			}
		}
		var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
		prefs.setCharPref('browser.startup.homepage', 'http://www.mangahere.co');
	} else {
		alert('This funciton is not supported by your brower, please add manually');
	}
}
