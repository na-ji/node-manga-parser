<script type="text/javascript">


if(!getCookie) {
    function setCookie(c_name,value,expiredays) {
        var exdate=new Date()
        exdate.setDate(exdate.getDate()+expiredays)
        cookieVal=c_name+ "=" +escape(value)+((expiredays==null) ? "" : ";expires="+exdate.toGMTString())+";domain=.www.mangahere.co;path=/";
        console.log(cookieVal);
        document.cookie=cookieVal;
    }
    function getCookie(c_name) {
        if (document.cookie.length>0)
        {
            c_start=document.cookie.indexOf(c_name + "=")
            if (c_start!=-1)
            {
                c_start=c_start + c_name.length+1
                c_end=document.cookie.indexOf(";",c_start)
                if (c_end==-1) c_end=document.cookie.length
                return unescape(document.cookie.substring(c_start,c_end))
            }
        }
        return ""
    }
}

function jumpMoible(){
	var web_url = 'http://www.mangahere.co/';
	var mobile_url = 'http://m.mangahere.co/';
	var mobile_url = 'http://m.mangahere.co';
	var local_url = document.location.href;
	if(/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
	        var hide = getCookie('MHNMM');
	        if(hide == "" || hide != 1) {
	        	if(document.location.href==web_url){
	        		document.location.href=mobile_url;
	        	}else{
	        		var path_url = document.location.pathname;
	        		if(path_url=='/'){
	        			var search = document.location.search;
	        			search = search.replace('?','');
	        			search = search.replace('#','');
	        			var search_params = search.split('&');
	        			for (var i = search_params.length - 1; i >= 0; i--) {
	        				if(search_params[i]=='f=mobile'){
	        					 setCookie("MHNMM", 1, null);
	        				}
	        			};

	        		}else if(/^(\/manga)\/([\w\-]+\/?)+/i.test(path_url)){
	        			document.location.href=mobile_url+path_url;
	        		}else if(/^(\/news)\/(\d+\/?)?/i.test(path_url)){
	        			document.location.href=mobile_url+'/news/';
	        		}else if(/^(\/news)\/([\w\-]+\/?)+/i.test(path_url)){
	        			document.location.href=mobile_url+path_url;
	        		}else if(/^(\/spoilers)\/(\d+\/?)?/i.test(path_url)){
	        			document.location.href=mobile_url+'/spoilers/';
	        		}else if(/^(\/spoiler)\/([\w\-]+\/?)+/i.test(path_url)){
	        			document.location.href=mobile_url+path_url;
	        		}
	        	}
	        }
	}
}
jumpMoible();

