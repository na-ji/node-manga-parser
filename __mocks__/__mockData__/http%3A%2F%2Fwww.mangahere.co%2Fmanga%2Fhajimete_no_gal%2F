<script type="text/javascript">
$LAB.queueWait(function(){
    //rating
    $(function(){
        rate(50);
        $("#rate").mouseout(function(){
            for(var i=1;i<=5;i++){
                $("#s"+i).removeClass();
                $("#s"+i).addClass("star-full");
            }
            rate(50);
            $("#current_rating").html("4<em>.82</em>");
        });
        var $div_li =$(".chapters_points a");
        $div_li.click(function(){
            $(this).addClass("select").siblings().removeClass("select");
            var index =  $div_li.index(this);
            $("div.detail_tablist > ul").eq(index).show().siblings().hide();
        });
        $(".star_write i").each(function(){
            $(this).mouseover(function(){
                for(var i=1;i<=5;i++){
                    $("#s"+i).removeClass();
                    $("#s"+i).addClass("star-empty");
                }
                var k = $(this).attr("gid");
                k = parseInt(k);
                for(var i=1;i<=k;i++){
                    $("#s"+i).removeClass();
                    $("#s"+i).addClass("star-full");
                }
                $("#current_rating").html(k+'<em>.00</em>');
            }).click(function(){
                var score = $(this).attr("gid");
                $.ajax({
                    type: "POST",
                    url: "/ajax/rating.php",
                    data:"score="+score+"&series_id=22545&user_id=0",
                    success: function(msg){alert(msg);}
                });
            })
        });
    });
    $("#bookmark").bind("click", function(){
        $.ajax({
            type: "POST",
            url: "/ajax/bookmark.php",
            data: "action=add&series_id=22545",
            success: function(msg){
                if(msg==1){alert("You need to login first!");location.href='/login.htm';}
                else{alert(msg);}
            }
        });
    });
        $("#upload").bind("click", function(){
        if(is_login){
            popUp(p_upload, 388, 400);
        }else{
            alert("You need to login first!");
            location.href='/login.htm';
        }
    });
    $("#subscribe").bind("click", function () {
    $('#email_post').show();
    $('#subemail').val('');
    $('#subemail').focus();
    });
  $("#subscribe_b").bind("click", function(){
        var email = $("#subemail")
        if(!email.val()) {
            alert('Please input an email address.');
            return false;
        }
        $.ajax({
            type: "POST",
            url: "/ajax/subscribe.php",
            data: "action=subscribe&series_id=22545&email="+email.val(),
            success: function (msg) {
                if (msg == 1) {
          email.focus();
          alert("Please input an email address.");
                } else if (msg == 2) {
          email.focus();
                    alert("Invalid email address!");
                } else {
          $('#email_post').hide();
                    alert(msg);
                }
            }
        });
  });
    $("#close_email_post").bind("click", function () {
    $('#email_post').hide();
    });
    $(window).scroll(function(a){
        var image_h = $("#main").height();
        y = $(window).scrollTop();
        if (y >= 0 && y< image_h ) {
            $('#detail_rightfb').css({ 'top':'0px', "position":"fixed" });
        }
        if (y < 1100 ) {
            $('#detail_rightfb').css({ 'top':'auto', "position":"static" });
        }
    });
});
function rate(rating) {
    var s,g;
    rating = rating.toString();
    if(rating=="0"){
        var j=1;
        while(j<=5){
            $("#s"+j).removeClass();
            $("#s"+j).addClass("star-empty");
            j++;
        }
    }else{
        s=rating.substr(0,1);
        g=rating.substr(1,1);
        for(var i=1;i<=parseInt(s);i++){
            $("#s"+i).removeClass();
            $("#s"+i).addClass("star-full");
        }
        if(parseInt(g)==5){
            $("#s"+i).removeClass();
            $("#s"+i).addClass("star-half");
            i++;
        }
        while(i<=5){
            $("#s"+i).removeClass();
            $("#s"+i).addClass("star-empty");
            i++;
        }
    }
}
function cut_hide() {
    $("#hide").show();
    $("#show").hide();
}
function cut_show() {
    $("#hide").hide();
    $("#show").show();
}
function popUp(a, w, h) {
    day = new Date();
    id = day.getTime();
//    window.open(a, id, 'toolbar=0,scrollbars=1,location=0,statusbar=1,menubar=0,resizable=0,width=' + w + ',height=' + h)
    window.location.href = a;
}
</script>
