$LAB.queueScript("http://www.mangahere.co/media/js/comments.js?v=20170616112357").queueWait(function () {
    iflogined = 0;
    $(function () {
        var $div_li = $(".tab_comment li");
        $div_li.click(function () {
            $(this).addClass("select").siblings().removeClass("select");
            var index = $div_li.index(this);
            if (index > 0 && !commentLoaded) {
                getfirst("manga", "22545");
                commentLoaded = true;
            }
            $("div.all_commet > div").eq(index).show().siblings().hide();
        });
    });
});
